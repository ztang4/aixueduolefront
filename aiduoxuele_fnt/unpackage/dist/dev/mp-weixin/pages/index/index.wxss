/* 水平间距 */
page {
  background-color: #F8F8F8;
}
.uni-noticebar__content-wrapper {
  padding-top: 20rpx !important;
  font-weight: 500;
  font-size: 28rpx;
  color: #222222;
}
.swiper-item {
  font-weight: 500;
  font-size: 28rpx;
  color: #222222;
  padding-left: 30rpx;
  line-height: 72rpx;
  max-width: 660rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.container {
  box-sizing: border-box;
}
.container .popupbox {
  width: 662rpx;
  height: 1000rpx;
  border-radius: 20rpx 20rpx;
  position: relative;
}
.container .popupbox .time2 {
  box-sizing: border-box;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  width: -webkit-fit-content;
  width: fit-content;
  padding: 10rpx;
  background-color: #999999;
  color: #ffffff;
  font-size: 24rpx;
  border-radius: 10rpx;
}
.container .popupbox .img1 {
  display: block;
  width: 100%;
  height: 350rpx;
}
.container .popupbox .img2 {
  display: block;
  width: 42rpx;
  height: 42rpx;
  margin: 0 auto;
  margin-top: 38rpx;
}
.container .popupbox .link {
  box-sizing: border-box;
  width: 100%;
  border-radius: 0 0 20rpx 20rpx;
  background-color: #ffffff;
  padding: 20rpx 0 20rpx 0;
}
.container .popupbox .link .link2 {
  box-sizing: border-box;
  width: 602rpx;
  height: 90rpx;
  background: #F2F2F2;
  border-radius: 20rpx 20rpx 20rpx 20rpx;
  line-height: 90rpx;
  padding: 0 20rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.container .popupbox .link .btn {
  width: 334rpx;
  height: 80rpx;
  background: #008CE9;
  border-radius: 68rpx 68rpx 68rpx 68rpx;
  text-align: center;
  line-height: 80rpx;
  font-weight: 500;
  font-size: 30rpx;
  color: #FFFFFF;
  margin: 0 auto;
}
.container .box {
  box-sizing: border-box;
  padding: 0 30rpx;
  margin-top: 30rpx;
}
.container .box .item2 {
  box-sizing: border-box;
  width: 100%;
  padding: 26rpx 24rpx;
  background-color: #ffffff;
  margin-bottom: 30rpx;
}
.container .box .item2 .b {
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.container .box .item2 .b .bi {
  display: flex;
  align-items: center;
  font-weight: 500;
  font-size: 26rpx;
  color: #222222;
  margin-left: 52rpx;
}
.container .box .item2 .b .bi .zan {
  width: 36rpx;
  height: 36rpx;
  margin-right: 8rpx;
}
.container .box .item2 .b .bi .pinglun {
  width: 36rpx;
  height: 36rpx;
  margin-right: 8rpx;
}
.container .box .item2 .sucai {
  width: 646rpx;
  height: 420rpx;
  border-radius: 20rpx;
  margin-bottom: 24rpx;
}
.container .box .item2 .sucai2 {
  width: 646rpx;
  height: 420rpx;
  border-radius: 20rpx;
  margin-bottom: 24rpx;
}
.container .box .item2 .head {
  display: flex;
  align-items: center;
  margin-bottom: 12rpx;
}
.container .box .item2 .head .avatar {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-right: 12rpx;
}
.container .box .item2 .head .hr {
  flex: 1;
}
.container .box .item2 .head .hr .name {
  font-weight: bold;
  font-size: 32rpx;
  color: #222222;
  margin-bottom: 8rpx;
}
.container .box .item2 .head .hr .time {
  font-weight: 500;
  font-size: 26rpx;
  color: #666666;
}
.container .box .item2 .content {
  font-weight: 500;
  font-size: 28rpx;
  color: #222222;
  margin-bottom: 20rpx;
}
.container .fun {
  width: 690rpx;
  background: #FFFFFF;
  border-radius: 20rpx 20rpx 20rpx 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-evenly;
  margin: 20rpx auto;
  padding: 30rpx 0;
}
.container .fun .item {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.container .fun .item .img {
  width: 92rpx;
  height: 92rpx;
}
.container .fun .item .txt {
  font-weight: 500;
  font-size: 30rpx;
  color: #222222;
  margin-top: 18rpx;
  text-align: center;
}
.container .notice {
  box-sizing: border-box;
  width: 100%;
  height: 72rpx;
  background: #FFEEEE;
  display: flex;
  align-items: center;
  padding: 0 28rpx;
}

