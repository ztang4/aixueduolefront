/* 水平间距 */
.container {
  box-sizing: border-box;
  width: 100%;
  height: 100vh;
}
.container .box {
  box-sizing: border-box;
  width: 690rpx;
  height: 760rpx;
  position: relative;
  margin: 0 auto;
  margin-top: 82rpx;
}
.container .box .codebox {
  box-sizing: border-box;
  width: 492rpx;
  height: 470rpx;
  border: 20rpx solid #5764E2;
  border-radius: 42rpx 42rpx 42rpx 42rpx;
  position: absolute;
  top: 194rpx;
  left: 116rpx;
  text-align: center;
  padding-top: 8rpx;
}
.container .box .codebox .img3 {
  width: 420rpx;
  height: 420rpx;
}
.container .box .img2 {
  width: 100%;
  height: 100%;
}
.container .nav {
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
  z-index: 99;
  top: 0;
  left: 0;
  padding: 0 20rpx;
}
.container .bgc {
  position: fixed;
  width: 100%;
  height: 100vh;
  top: 0;
  left: 0;
}

