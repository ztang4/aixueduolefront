/* 水平间距 */
page {
  background-color: #F8F8F8;
}
.container {
  box-sizing: border-box;
}
.container .namebox {
  box-sizing: border-box;
  width: 642rpx;
  height: 366rpx;
  background: #FFFFFF;
  border-radius: 20rpx 20rpx 20rpx 20rpx;
  padding: 40rpx 0 0 0;
}
.container .namebox .btns {
  display: flex;
  align-items: center;
  border-top: 1rpx solid #E7E7E7;
  margin-top: 50rpx;
}
.container .namebox .btns .btn1 {
  width: 49%;
  height: 100rpx;
  border-right: 1rpx solid #E7E7E7;
  text-align: center;
  line-height: 100rpx;
  font-weight: 500;
  font-size: 30rpx;
  color: #666666;
}
.container .namebox .btns .btn2 {
  width: 50%;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  font-weight: 500;
  font-size: 30rpx;
  color: #0779EA;
}
.container .namebox .default {
  font-weight: 500;
  font-size: 30rpx;
  color: #666666;
}
.container .namebox .title {
  font-weight: bold;
  font-size: 32rpx;
  color: #222222;
  text-align: center;
}
.container .namebox .ipt {
  box-sizing: border-box;
  width: 558rpx;
  height: 100rpx;
  background: #F2F2F2;
  border-radius: 20rpx 20rpx 20rpx 20rpx;
  padding: 0 50rpx;
  margin: 0 auto;
  margin-top: 32rpx;
}
.container .box1 {
  box-sizing: border-box;
  margin: 0 auto;
  margin-top: 50rpx;
  text-align: center;
  position: relative;
  width: 144rpx;
  height: 144rpx;
  border-radius: 72rpx 72rpx 72rpx 72rpx;
}
.container .box1 .cream {
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 48rpx;
  position: absolute;
  bottom: 12rpx;
  right: 12rpx;
  background-color: #ffffff;
}
.container .name {
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 690rpx;
  height: 120rpx;
  background: #FFFFFF;
  border-radius: 20rpx 20rpx 20rpx 20rpx;
  padding: 0 40rpx;
  margin: 0 auto;
  margin-top: 100rpx;
}

